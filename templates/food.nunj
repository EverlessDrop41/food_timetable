{% extends "layout.nunj" %}
{% block body %}
    <h2>A list of food items</h2>

    <div id="foodListVue">
      {% raw %}
      <ul>
        <li v-for="f in food">
          <span class="foodName">{{f.name}}</span> <span class="foodCost">{{parseCost(f.price)}}</span>
        </li>
      </ul>
      {% endraw %}
    </div>
{% endblock %}
{% block scripts %}
  <script src="/static/js/utils.js"></script>
  <script src="/static/js/food.js"></script>
{% endblock %}
